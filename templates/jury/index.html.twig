{% extends 'base.html.twig' %}

{% block title %}Jury index{% endblock %}

{% block body %}
    <h1>Jury index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for jury in juries %}
            <tr>
                <td>{{ jury.id }}</td>
                <td>
                    <a href="{{ path('app_jury_show', {'id': jury.id}) }}">show</a>
                    <a href="{{ path('app_jury_edit', {'id': jury.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_jury_new') }}">Create new</a>
{% endblock %}
